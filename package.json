{
	"name": "agentfs-opencode",
	"version": "0.1.0",
	"module": "src/index.ts",
	"type": "module",
	"main": "dist/index.js",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "bun build src/index.ts --outdir dist --target bun",
		"test": "bun test",
		"typecheck": "tsc --noEmit",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"check": "biome check . && tsc --noEmit && bun test",
		"link": "bun run build && bun link && cd .opencode && bun link agentfs-opencode"
	},
	"dependencies": {
		"@opencode-ai/plugin": "^1.0.195",
		"agentfs-sdk": "^0.3.1",
		"zod": "^4.2.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.10",
		"@types/bun": "latest",
		"typescript": "^5.9.3"
	},
	"peerDependencies": {
		"typescript": "^5"
	}
}
